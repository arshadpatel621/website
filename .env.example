# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Server Configuration
PORT=3000
NODE_ENV=development

# Security (generate your own secret key)
JWT_SECRET=your_jwt_secret_key_here

# Email Configuration for Consultation Notifications
# Your email address (Gmail, Outlook, etc.)
EMAIL_USER=your-email@gmail.com

# App password or email password
# For Gmail: Use App Password (not your regular password)
# Go to: Google Account > Security > 2-Step Verification > App passwords
EMAIL_PASS=your-app-password-here

# Email address where you want to receive consultation notifications
# (Can be the same as EMAIL_USER or different)
NOTIFICATION_EMAIL=arshadpatel1431@gmail.com

# Example:
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# EMAIL_USER=your-email@gmail.com
# EMAIL_PASS=your-16-char-app-password
# NOTIFICATION_EMAIL=arshadpatel1431@gmail.com
